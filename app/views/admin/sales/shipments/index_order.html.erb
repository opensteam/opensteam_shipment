<% render_headline 'categories_icon', "Order ##{@order.id} - Shipments", 'content-header/headline_icon.jpg' %>
<% render_header_buttons 'New Shipment', :href => new_admin_sales_order_shipment_path( @order ) %>

<div id="editor-wrapper">
	<table cellpadding="0" cellspacing="0">
		<tr>
			<td width="200">
				<%= order_navigation( @order, :active => :shipments ) %>
			</td>
			<td width="20"><br /></td>
			<td width="100%" id="editor_content">
				<%= render :partial => "admin/sales/shipments/shipments", :object => @shipments %>
			</td>
		</tr>
	</table>
</div>